# Flag to enable a local ONNX model
EMBEDDING_MODEL_LOCAL_ENABLED=true
# Path to the local ONNX embedding model (only considered if EMBEDDING_MODEL_LOCAL_ENABLED=true)
EMBEDDING_MODEL_LOCAL_DIR=/path/to/your/model/directory
EMBEDDING_MODEL_LOCAL_FILE_NAME=model.onnx
EMBEDDING_MODEL_LOCAL_TOKENIZER_FILE_NAME=tokenizer.json

# Flag to enable a HuggingFace model via the Inference API
EMBEDDING_MODEL_HUGGINGFACE_ENABLED=false
# HuggingFace Inference API settings (only considered if EMBEDDING_MODEL_HUGGINGFACE_ENABLED=true)
EMBEDDING_MODEL_HUGGINGFACE_MODEL_NAME=mixedbread-ai/deepset-mxbai-embed-de-large-v1
EMBEDDING_MODEL_HUGGINGFACE_API_KEY=your-api-key

# LLM settings
LLM_API_KEY=api-key
LLM_BASE_URL=https://model-base-url.com
LLM_MODEL_NAME=model-name
LLM_MAX_CHAT_HISTORY=10